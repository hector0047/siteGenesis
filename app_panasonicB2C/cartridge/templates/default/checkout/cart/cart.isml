										<isif condition="${FormCoupon.object.applied || (FormCoupon.object.statusCode==dw.campaign.CouponStatusCodes.NO_APPLICABLE_PROMOTION && FormCoupon.object.bonusDiscountLineItems.size()>0)}">
												<span class="bonus-item">${Resource.msg('global.applied','locale',null)}</span>
											<iselse/> 
												<span class="bonus-item couponerror">${Resource.msg('global.notapplied','locale',null)}</span>
											</isif>
									</div>
								</isloop>
							</div>
						</div>
					</div>
					
					<div class="cart-actions">
						<div class="cart-action-left">
						<iscontentasset aid="account-nav-text-prechat"/>
						</div>
					</div>
					
					</div>
					
					<div id="sidebar" class="cart-summery-container">
						<div class="menu css3transition">
							<div class="cart-summery mobile-summary">
								<h2>Summary</h2>
								<div class="cart-order-totals">
										<isordertotals p_lineitemctnr="${pdict.Basket}" p_totallabel="${Resource.msg('global.estimatedtotal','locale',null)}" p_cartpage="${true}" p_shippingpage="${false}"/>						
								</div>
								<div class="cart-action-right">
								<iscomment>continue shop url is a non-secure but checkout needs a secure and that is why separate forms!</iscomment>
								<form class="cart-action-checkout" action="${URLUtils.httpsContinue()}" method="post" name="${pdict.CurrentForms.cart.dynamicHtmlName}" id="checkout-form">
									<fieldset>
										<isif condition="${enableCheckout && !empty(pdict.Basket.productLineItems) }">
											<button class="blue-button" type="submit" value="${Resource.msg('global.checkout','locale',null)}" name="${pdict.CurrentForms.cart.checkoutCart.htmlName}">
												${Resource.msg('cart.checkout','locale',null)}
											</button>
										</isif>
									</fieldset>
								</form>		 
								<iscomment>Added for paypal</iscomment>	
								<isif condition="${dw.system.Site.getCurrent().getCustomPreferenceValue('EnablePayPal') && (enableCheckout && !empty(pdict.Basket.productLineItems))}">
									<isinclude template="includes/cartinclude" />
								</isif>	
								</div>
							</div>
								<div class="cart-summey-fetures">
									<ul>
									<li><div class="cart-fetures-icon1"></div><p>FREE Standard Shipping</p></li>
									<li><div class="cart-fetures-icon2"></div><p>Easy Returns <a href="#" class="easyreturns">Click for details</a></p></li>
									<li><div class="cart-fetures-icon3"></div><p>Fast, Secure Checkout</p></li>
									</ul>
									<div class="easyreytutrns-detaisl hide"> 
										<iscontentasset aid="carteasyreturn-details" />
									</div>
								</div>
						</div>
					</div>
					<div class="mobileBlock">
						<div class="cart-actions mobileCartActions">
							<div class="cart-action-left">
							<iscontentasset aid="account-nav-text-prechat"/>
							</div>
						</div>
					</div>
					<script>
				     $(document).on('ready', function() {
					     if($(window).width() < 1025)
							{
					
							} else {
								 //Desktop Checkout Sticky
								 $.lockfixed("#sidebar .menu",{offset: {top: 10, bottom: 900}});	
							}
				    });
					</script>
					<iscomment>
						JIRA PREV-45 : Able to proceed to checkout flow with more than instock qty by clicking on Go straight to checkout in the Mini cart.
						Commented second error message.
						<isif condition="${!enableCheckout}">
							<div class="error-message">
								${Resource.msg('cart.limited','checkout',null)}: 
								${Resource.msg('cart.limitedQuantity','checkout',null)}
							</div>
						</isif>
					</iscomment>
				</fieldset>
			</form>
			<div class="clearfix"></div>
			</isif>
			<div class="cart-recent-views">
				<isinclude url="${URLUtils.url('Product-RecentlyViewedProducts')}"/>
			</div>
		 
			<iscomment><div class="cart-footer">
				<isslot id="cart-footer" description="Footer for Cart page" context="global" />
			</div></iscomment>		
		</isif>
	</isif>
	
</isdecorate>
