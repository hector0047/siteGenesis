<iscontent type="text/html" charset="UTF-8" compact="true"/>
<isscript>
	importScript("int_dis:product/ProductImageSO.ds");
	importScript("util/GetAmplienceImageUrl.ds");
</isscript>
<iscomment>FArhan's Dev team CartUpdate Whole file is chenged 15/1/2016 7:50pm</iscomment> 	

<isset name="PageName" value="CartPage" scope="page"/>
<isdecorate template="checkout/cart/pt_cart">
	<isinclude template="util/modules"/>
	<isinclude template="util/reporting/ReportBasket.isml" />
	<isset name="recycleProductID" value="${('recycleProductID' in dw.system.Site.current.preferences.custom && dw.system.Site.current.preferences.custom.recycleProductID) ? dw.system.Site.current.getCustomPreferenceValue('recycleProductID').trim() : ''}" scope="page" />
	<isset name="enableCheckout" value="${pdict.EnableCheckout}" scope="page" />
	<isif condition="${dw.system.Site.getCurrent().getCustomPreferenceValue('enableStorePickUp')}">
		<isset name="store" value="${dw.catalog.StoreMgr.getStore(pdict.CurrentSession.custom.storeId)}" scope="page" />
</isif>
	<isif condition="${!(pdict.CurrentCustomer.registered) && (!pdict.CurrentCustomer.authenticated)}">	
		<isinclude template="account/login/cartlogin.isml"/>
	</isif>
	<iscomment><isslot id="cart-banner" description="Banner for Cart page"	context="global" /></iscomment>
	<h1>${Resource.msg('cart.header','checkout',null)}</h1>
	<isif condition="${pdict.Basket == null || (empty(pdict.Basket.productLineItems) && empty(pdict.Basket.giftCertificateLineItems))}">
	<div class="cart-empty">
			${Resource.msg('cart.cartempty','checkout',null)}
	</div> 
	</isif>
	</div>	
	<div class="cart-top-actions cart-continue-action">		
			<span></span><form class="cart-action-continue-shopping" action="${URLUtils.httpContinue()}" method="post" name="${pdict.CurrentForms.cart.dynamicHtmlName}" id="continue-shopping">
				<fieldset>
					<button class="" type="submit" value="${Resource.msg('global.continueshopping','locale',null)}" name="${pdict.CurrentForms.cart.continueShopping.htmlName}">${Resource.msg('global.continueshopping','locale',null)}</button>
				</fieldset>
			</form>
	</div>	













<div id="sticky-anchor"></div>
<div id="cartstickyCheckout">
<div class="mobileBlock">
<div class="mobileCartAction">
	<div class="cart-action-right">
	<iscomment>continue shop url is a non-secure but checkout needs a secure and that is why separate forms!</iscomment>
	<form class="cart-action-checkout" action="${URLUtils.httpsContinue()}" method="post" name="${pdict.CurrentForms.cart.dynamicHtmlName}" id="checkout-form">
		<fieldset>
			<isif condition="${enableCheckout && !empty(pdict.Basket.productLineItems) }">
				<button class="blue-button" type="submit" value="${Resource.msg('global.checkout','locale',null)}" name="${pdict.CurrentForms.cart.checkoutCart.htmlName}">
					${Resource.msg('cart.checkout','locale',null)}
				</button>
			</isif>
		</fieldset>
	</form>		 
	<iscomment>Added for paypal</iscomment>	
	<isif condition="${dw.system.Site.getCurrent().getCustomPreferenceValue('EnablePayPal') && (enableCheckout && !empty(pdict.Basket.productLineItems))}">
		<isinclude template="includes/cartinclude" />
	</isif>	
	</div>
</div>
</div>			
</div>
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
	<isif condition="${pdict.Basket != null || (pdict.Basket.productLineItems !=null) && !empty(pdict.Basket.giftCertificateLineItems)}">
		<iscomment>Display approaching order and shipping promotions.</iscomment>
		<div class="cart-top-promotional-offer"><isinclude template="checkout/components/approachingdiscounts"/></div>
		<iscomment>
		<div class="cart-top-actions cart-checkout-action">
				continue shop url is a non-secure but checkout needs a secure and that is why separate forms!
				<form class="cart-action-checkout" action="${URLUtils.httpsContinue()}" method="post" name="${pdict.CurrentForms.cart.dynamicHtmlName}" id="checkout-form">
					<fieldset>
						<isif condition="${enableCheckout && !empty(pdict.Basket.productLineItems)}">
							<button class="blue-button" type="submit" value="${Resource.msg('global.checkout','locale',null)}" name="${pdict.CurrentForms.cart.checkoutCart.htmlName}">
								${Resource.msg('global.checkout','locale',null)}
							</button>
						</isif>
					</fieldset>
				</form>
		</div>
		</iscomment>
	</isif>
	<iscomment>Display selected bonus products Order Promotion message</iscomment>

	<isif condition="${!(pdict.Basket == null)}"> 
		<isloop items="${pdict.Basket.bonusDiscountLineItems}"	var="bonusDiscountLineItem">
			<isif condition="${bonusDiscountLineItem.getPromotion().getPromotionClass() == dw.campaign.Promotion.PROMOTION_CLASS_ORDER && bonusDiscountLineItem.getBonusProductLineItems().size() == 0}">
				<div class="bonus-item-promo">

					<iscomment>Display appropriate text based on status of bonus discount and number of selected bonus products.</iscomment> 
						<isif condition="${bonusDiscountLineItem.getMaxBonusItems() > 1}">
							<isset name="bonusButtonText" value="${Resource.msg('cart.selectbonusproducts','checkout',null)}" scope="page" />
						<iselse/>
							<isset name="bonusButtonText" value="${Resource.msg('cart.selectbonusproduct','checkout',null)}" scope="page" />
						</isif>
								<isif condition="${bonusDiscountLineItem.getBonusProductLineItems().size() > 0}">
									<isset name="bonusButtonDisabled" value="${true}" scope="page" />
								<iselse/>
									<isset name="bonusButtonDisabled" value="${false}" scope="page" />
					</isif>
					<div class="bonus-item-details">
						<isprint value="${bonusDiscountLineItem.getPromotion().getName()}" />
						<isprint value="${bonusDiscountLineItem.getPromotion().getCalloutMsg()}" encoding="off"/>
						<isif condition="${!empty( bonusDiscountLineItem.getPromotion().getDetails() )}">
							<a title="${bonusDiscountLineItem.getPromotion().getDetails().markup}">
								${Resource.msg('promo.seedetail','checkout',null)}
							</a>
						</isif>
						<iscomment>
							<a class="tooltip" href="#">
								${Resource.msg('product.promotiondetails','product',null)}
								<div class="tooltip-content" data-layout="small">
									<isprint value="${bonusDiscountLineItem.getPromotion().getDetails()}" />
									<br />
									${StringUtils.format(Resource.msg('cart.bonusmaxitems','checkout',null),bonusDiscountLineItem.getMaxBonusItems())}
									${StringUtils.format(Resource.msg('cart.bonusnumselected','checkout',null),bonusDiscountLineItem.getBonusProductLineItems().size())}
								</div>
							</a>
						</iscomment>
					</div>
					<iscomment>
					<div class="bonus-products">
						<a href="${URLUtils.url('Product-GetBonusProducts','bonusDiscountLineItemUUID', bonusDiscountLineItem.UUID)}" title="${bonusButtonText}">
							${bonusButtonText}
						</a>
					</div>
					</iscomment>
				</div>
			</isif>
		</isloop>
	</isif>
	<isif condition="${!(pdict.Basket == null)}">
		<isif condition="${!empty(pdict.Basket) || (!empty(pdict.Basket.productLineItems) && !empty(pdict.Basket.giftCertificateLineItems))}">
			<iscomment>prevent checkout if any product price is N/A or if invalid coupons</iscomment>
			<isif condition="${pdict.BasketStatus != null && pdict.BasketStatus.error}">
				<div class="cart-error-msg error-form">
					<isif condition="${pdict.BasketStatus.code != null && pdict.BasketStatus.code=='CouponError'}">
						${Resource.msg('cart.cartcouponinvalid','checkout',null)}
					<iselseif condition="${pdict.BasketStatus.code != null && pdict.BasketStatus.code=='TaxError'}">
						${Resource.msg('cart.taxinvalid','checkout',null)} 
					<iselse/>
						Error: ${pdict.BasketStatus.code}
						${Resource.msg('cart.carterror','checkout',null)}
					</isif>
				</div>
			</isif>
			<isif condition="${dw.system.Site.getCurrent().getCustomPreferenceValue('EnablePayPal')}">
				<isif condition="${pdict.PaypalError != null}">
					<div class="error-form ">
						${Resource.msg('confirm.error.declined','checkout',null)} 
					</div>
				</isif>
			</isif>
	
			<iscomment>show the basket when we have data</iscomment>
			<isif condition="${!empty(pdict.Basket.productLineItems) }">
			<form action="${URLUtils.httpContinue()}" method="post"	name="${pdict.CurrentForms.cart.htmlName}" id="cart-items-form">
				<fieldset>
					<iscomment>This button is hidden but required to ensure that update cart is called whenever the "enter" key is pressed in an input field</iscomment>
					<iscomment> 
						<button class="visually-hidden" type="submit" value="${pdict.CurrentForms.cart.updateCart.htmlName}" name="${pdict.CurrentForms.cart.updateCart.htmlName}"></button>
					</iscomment>
					<div class="cart-list-coupon-wrapper cart-listMobDvc">
					<table id="cart-table" class="item-list">		
						<thead>
							<tr>
								<th class="section-header section-product-cont" colspan="2">${Resource.msg('global.product','locale',null)}</th>
								<isif condition="${dw.system.Site.getCurrent().getCustomPreferenceValue('enableStorePickUp')}">
								<th class="section-header">${Resource.msg('cart.store.deliveryoptions','storepickup',null)}</th>
								</isif>
								<th class="section-header section-qty"><span>${Resource.msg('global.qty','locale',null)}</span></th>
								<iscomment><th class="section-header section-price"><span>${Resource.msg('global.unitprice','locale',null)}</span></th></iscomment>
								<th class="section-header header-total-price">${Resource.msg('global.price','locale',null)}</th>
							</tr>
						</thead>
			
						<tbody>
			
			
							<iscomment>	LOOP TO SEPARATE DIFFERENT SHIPMENTS ex: Gift Registry Shipments Etc.</iscomment>
							<isloop items="${pdict.CurrentForms.cart.shipments}" var="Shipment"	status="loopstate">
								<isloop items="${Shipment.items}" alias="FormLi" status="loopstate">
									<isset name="lineItem" value="${FormLi.object}" scope="page" />
									<isscript>
										var isInWishList = false; 
										if (pdict.WishList) { 
											var wishListItems =	pdict.WishList.items; 
											var wishListItemsSize = wishListItems.size();
											for each( var wishListItem in wishListItems ) {
												if (lineItem.productID	== wishListItem.product.ID) {
													isInWishList = true; 
													break; 
												} 
											} 
										}
									</isscript>
			
			
									<iscomment>	Skip bonus-choice products in first pass.	</iscomment>
									<isif condition="${(lineItem.bonusDiscountLineItem == null) &&(lineItem.product.ID != recycleProductID)}">
										<tr class="cart-row">
											<td class="item-image">
												<isif condition="${lineItem.product != null}">
												<isset name="image" value="${new ProductImage('image150X150',lineItem.product,0)}" scope="page"/>
												<a href="${URLUtils.https('Product-Show','pid',lineItem.product.ID)}">
												
												<!-- img src="${getAmplienceImageUrl(image.getURL(),'$Cart-Large-Thumbnail-Desktop$')}"	alt="${image.alt}" title="${lineItem.product.ID}" / -->
												<picture>
													<source srcset="${getAmplienceImageUrlViewPortWise(image.getURL(),'$Cart-Large-Thumbnail-Desktop$','desktop')}" media="(min-width: 1024px)">
													<source srcset="${getAmplienceImageUrlViewPortWise(image.getURL(),'$Cart-Large-Thumbnail-Desktop$','tablet')}" media="((min-width: 768px) and (max-width: 1023px))">
													<source srcset="${getAmplienceImageUrlViewPortWise(image.getURL(),'$Cart-Large-Thumbnail-Desktop$','mobile')}" media="(max-width: 767px)">
													<img src="${getAmplienceImageUrlViewPortWise(image.getURL(),'$Cart-Large-Thumbnail-Desktop$','desktop')}" alt="${image.alt}" title="${lineItem.product.ID}" />
												</picture>
												</a>
												<iselse/>
													<img src="${URLUtils.staticURL('/images/noimagesmall.png')}" alt="${lineItem.productName}" title="${lineItem.productName}" />
												</isif> 
												<isif condition="${lineItem.bonusProductLineItem}">
													<span class="bonus-item">
														${Resource.msg('global.bonus','locale',null)}
													</span>
												</isif>
											</td>
											<td class="item-right-details" colspan="4">
												<table>
													<tr>
														<td class="item-details">
															<iscomment>Call module to render product</iscomment> 
															<div class="block1">
																<isdisplayliproduct p_productli="${lineItem}" p_formli="${FormLi}" p_editable="${true}" p_hideprice="${true}" p_hidepromo="${false}" />
															</div>
															<div class="block2">
																<iscomment>Product Existence and Product Availability</iscomment>
																<isif condition="${!lineItem.bonusProductLineItem || lineItem.getBonusDiscountLineItem() != null}">
																	<isif condition="${lineItem.product == null}"> 
																		<span class="not-available">
																			${Resource.msg('cart.removeditem','checkout',null)}
																		</span>
																	<iselse/>
																		<isset name="product" value="${lineItem.product}" scope="page" /> 
																		<isset name="quantity"	value="${pdict.Basket.getAllProductQuantities().get(lineItem.product).value}" scope="page" />
																		<isinclude template="checkout/cart/cartavailability" />
																	</isif>
																</isif>
																
																<iscomment>Displaying the shipment detail
																<isif condition="${lineItem.product.primaryCategory !=null && lineItem.product.primaryCategory.ID == 'warranty'}">
																	<div class="default-shippingmethod-msg">
																		<isprint value="${dw.system.Site.current.preferences.custom.customerCarePlanShippingMessage}" encoding="off" />
																	</div>
																<iselse/>
																	<div class="default-shippingmethod-msg">
																		<iscomment>Displaying estimated ship date
																		<isministockmessage product="${lineItem.product}"/>
																		<isset name="defaultShipping" value="${false}" scope="page"/>
				  														<isloop items="${dw.order.ShippingMgr.getProductShippingModel(lineItem.product).getApplicableShippingMethods()}" var="shippingObj" status="loopstate">
				 															 <isif condition="${shippingObj.isDefaultMethod()}">
				 															 	<isset name="defaultShipping" value="${true}" scope="page"/>
																		  		<isset name="defaultShippingDescription" value="${shippingObj.getDescription()}" scope="page"/>
																		  		<isset name="defaultShippingName" value="${shippingObj.getDisplayName()}" scope="page"/>
																		  	</isif>
																		</isloop>
																		
																		<isprint value="${dw.order.ShippingMgr.getProductShippingModel(lineItem.product).getApplicableShippingMethods()[0].getDisplayName()}"/>
																		
																	</div>
																</isif>	</iscomment>
																<isset name="promos" value="${dw.campaign.PromotionMgr.activeCustomerPromotions.getProductPromotions(lineItem.product)}" scope="page"/>
																<div class="promo-adjustment-container">
																	<isloop	items="${promos}" var="pa" status="prAdloopstatus">
																		<div class="promo-adjustment">
																			<isif condition="${!empty(pa.calloutMsg)}"> 
																				<isprint value="${pa.calloutMsg}" encoding="off"/>
																				<isif condition="${pdict.CurrentCustomer.authenticated && !(pdict.CurrentCustomer.isMemberOfCustomerGroup('EPP'))}"> 
																					<isif condition="${!empty(pa.details)}"> <a title="${pa.details}">${Resource.msg('promo.seedetail','checkout',null)} </a></isif>
																				<iselse/>
																					<isif condition="${!empty(pa.details)}"> <a title="${pa.details}">${Resource.msg('promo.seedetail','checkout',null)} </a></isif>
																				</isif>
																			</isif>
																		</div>
																	</isloop>
																</div>				
															</div>
														</td>
														<isif condition="${dw.system.Site.getCurrent().getCustomPreferenceValue('enableStorePickUp')}">
															<td class="item-delivery-options">
																<isinclude template="checkout/cart/storepickup/deliveryoptions" />
															</td>
														</isif>
														<td class="item-quantity">
														 <div class="item-quantity-cont">
														 
														 <div class="mobileCartQuntity mobileBlock"><isif condition="${lineItem.quantity.value.toFixed()>1}"><button name="${FormLi.subtractCartqty.htmlName}" >-</button></isif></div> 	
															<div>
															<isif condition="${lineItem.bonusProductLineItem}"> 
																<isprint value="${lineItem.quantity}" /> 
															<iselse>
																<isif condition="${(lineItem.product.primaryCategory !=null && lineItem.product.primaryCategory.ID == 'warranty')}">
																    <input type="text" name="${FormLi.quantity.htmlName}" size="4" maxlength="3" value="${lineItem.quantity.value.toFixed()}" class="input-text positivenumber" disabled="true"/>
																<iselse>
																	<input type="text" name="${FormLi.quantity.htmlName}" size="4" maxlength="3" value="${lineItem.quantity.value.toFixed()}" class="input-text positivenumber" />
																</isif>
															</isif>
															</div>
																<div class="mobileCartQuntity mobileCartQuntityplus mobile-text"><button name="${FormLi.addCartqty.htmlName}" >+</button></div>
																<div><isif condition="${!(lineItem.product.primaryCategory !=null && lineItem.product.primaryCategory.ID == 'warranty')}">
																	<button class="cart-product-update" name="${FormLi.updateCart.htmlName}" >
						                                                 ${Resource.msg('global.update','locale',null)}
						                                            </button>
																</isif>
																<isif condition="${!lineItem.bonusProductLineItem || lineItem.getBonusDiscountLineItem() != null}">							
																	<button type="submit" class="cart-product-remove" value="${Resource.msg('global.remove','locale',null)}" name="${FormLi.deleteProduct.htmlName}"><span class="mobile-text">X</span> <span>${Resource.msg('global.remove','locale',null)}</span></button>																				
																</isif></div>
				                                          </div>  
														</td>
														<iscomment>
														<td class="item-quantity-details">
															
															<div class="item-user-actions">
																<isif condition="${empty(pdict.ProductAddedToWishlist) || pdict.ProductAddedToWishlist != lineItem.productID}">
																	<isif condition="${!isInWishList}">
																		<a class="add-to-wishlist" href="${URLUtils.https('Cart-AddToWishlist', 'pid', lineItem.productID)}" name="${FormLi.addToWishList.htmlName}" title="${Resource.msg('global.addtowishlist','locale',null)}">
																			${Resource.msg('global.addtowishlist','locale',null)}
																		</a>
																	</isif>
																<iselse/>
																	<div class="in-wishlist">
																		${Resource.msg('global.iteminwishlist','locale',null)}
																	</div>
																</isif>
														 
															</div>
														</td>
													
														<td class="item-price">
															<div class="item-price-cont">
															<isif condition="${lineItem.product != null}">
						
																
																	StandardPrice: quantity-one unit price from the configured list price
																	book. SalesPrice: product line item base price. If these are
																	different, then we display crossed-out StandardPrice and also
																	SalesPrice. 
																
																
						
																Get the price model for this	product.
																<isset name="PriceModel" value="${lineItem.product.getPriceModel()}" scope="page" />
																
						
																Get StandardPrice from list price book.
																<isinclude template="product/components/standardprice" />
																
						
																Get SalesPrice from line item itself.
																<isset name="SalesPrice" value="${lineItem.basePrice}" scope="page" />
																<isif condition="${StandardPrice.available && StandardPrice > SalesPrice}">
																	StandardPrice and SalesPrice are different, show standard
																	<div class="price-promotion">
																		<span class="price-standard"><isprint value="${StandardPrice}" /></span>
																		<span class="price-sales"><isprint value="${SalesPrice}" /></span>
																	</div>
																<iselse/>
																	<span class="price-sales"><isprint value="${SalesPrice}" /></span>
																</isif>
																
																											
															</isif>
															</div>
														</td></iscomment>
						
														<td class="item-total">
														
															<isif condition="${lineItem.bonusProductLineItem}">
																<span class="bonus-item">
																	${Resource.msg('global.bonus','locale',null)}
																</span>
															<iselse/>											
																<iscomment>Display the unadjusted price if the line item has price adjustments.</iscomment>
																<isif condition="${lineItem.priceAdjustments.length > 0}">
																	<iscomment>Display	the adjusted item total.</iscomment> 
																	<span class="price-adjusted-total">
																		<iscomment>${Resource.msg('cart.itemtotal','checkout',null)}</iscomment>
																		<span class="cartsale">${Resource.msg('cart.sale','checkout',null)}</span><span><isprint value="${lineItem.getAdjustedPrice()}" /></span>
																	</span>
																	<div class="price-unadjusted">
																		<iscomment>${Resource.msg('cart.listprice','checkout',null)}</iscomment>
																		<span class="price-value"><isprint value="${lineItem.getPrice()}" /> </span>
																	</div>
																	<div class="adjust-save">
																		<isif condition="${lineItem.quantity> 1}">
																			<isset name="savedvalue" value="${((lineItem.getPrice()).subtract(lineItem.getAdjustedPrice()))}" scope="page" />
																			<isset name="savedvalueeach" value="${(lineItem.basePrice)}" scope="page" />
																			<div class="savePrice"><span class="price-yousaved">${Resource.msg('cart.yousave','checkout',null)}</span><span class="saveprice-value"><isprint value="${savedvalue.toFormattedString()}" formatter="#00.00#"/></span></div>
																			<div class="eacehPrice"><span class="price-yousaved-single">${Resource.msg('cart.yousaveForeach','checkout',null)}</span><span class="saveprice-value saveprice-value-single"><isprint value="${savedvalueeach.toFormattedString()}" formatter="#00.00#"/></span></div>
																		<iselse/>
																			<isset name="savedvalue" value="${(lineItem.getPrice()).subtract(lineItem.getAdjustedPrice())}" scope="page" />
																			<div class="savePrice"><span class="price-yousaved">${Resource.msg('cart.yousave','checkout',null)}</span><span class="saveprice-value"><isprint value="${savedvalue.toFormattedString()}" formatter="#00.00#"/></span></div>
																		</isif>
																	</div>
	                                                               
																	
																	<iscomment>Display the promotion name for each price adjustment.
																		<isloop	items="${lineItem.priceAdjustments}" var="pa" status="prAdloopstatus">
																			<div class="promo-adjustment">
																				<isif condition="${lineItem.quantityValue > 1 && lineItem.quantityValue != pa.quantity}">
																					<isprint value="${pa.quantity}" /> x 
																				</isif>
																				<isif condition="${!empty(pa.promotion.calloutMsg)"> 
																					<isprint value="${pa.promotion.calloutMsg}" encoding="off"/>
																				<iselse/>
																					<isprint value="${pa.promotion.ID}" encoding="off"/><a title=<isif condition="${!empty(pa.promotion.details)}"> "${pa.promotion.details}"</isif>>${Resource.msg('promo.seedetail','checkout',null)}</a>
																				</isif>
																				
																			</div>
																		</isloop></iscomment>
																<iselse/>
																
																	<iscomment>Display non-adjusted item total.</iscomment> 
																	<div>
																		<isif condition="${lineItem.quantity> 1}">
																			<span class="price-total">
																				<isprint value="${lineItem.getAdjustedPrice()}" />
																			</span>
																			<div><span class="saveprice-value saveprice-value-single"><isprint value=${lineItem.basePrice.toFormattedString()} /></span> <span class="price-yousaved-single">${Resource.msg('cart.yousaveForeach','checkout',null)}</span> </div>
																		<iselse/>
																			<span class="price-total">
																				<isprint value="${lineItem.getAdjustedPrice()}" />
																			</span>
																		</isif>
																		
																		
																	</div>
																</isif>
															</isif>
						
															<iscomment>Options</iscomment>
															<isif condition="${lineItem.optionProductLineItems.size() > 0}">
